<?xml version="1.0" encoding="utf-8" ?>
<OperationDefinitionCollection>
	<Items>
		<OperationDefinition>
			<RequestType>GetListItems</RequestType>
			<ListTitle>Projects</ListTitle>
			<TableName>Projects</TableName>
			<SiteUrl>[SRA Root]</SiteUrl>
			<CAML>
				<![CDATA[
					<Query>
						<Where>
							<Eq>
								<FieldRef Name="URL"/>
								<Value Type="Text">%SiteURL%</Value>
							</Eq>
						</Where>
					</Query>
					<ViewFields>
						<FieldRef Name="MapID"/>
					</ViewFields>
					<QueryOptions>
						<ViewFieldsOnly>True</ViewFieldsOnly>
						<DatesInUtc>True</DatesInUtc>
					</QueryOptions>
				]]>
			</CAML>
		</OperationDefinition>
		<OperationDefinition>
			<RequestType>GetListItems</RequestType>
			<ListTitle>MapStages</ListTitle>
			<SiteUrl>[SRA Root]</SiteUrl>
			<TableName>MapStages</TableName>
			<CAML>
				<![CDATA[
					<ViewFields>
						<FieldRef Name="ID"/>
						<FieldRef Name="Map"/>
						<FieldRef Name="Stage"/>
						<FieldRef Name="MapID"/>
						<FieldRef Name="StageID"/>						
						<FieldRef Name="StagePosition"/>						
						<FieldRef Name="Tollgate"/>						
					</ViewFields>
					<QueryOptions>
						<ViewFieldsOnly>True</ViewFieldsOnly>
						<DatesInUtc>True</DatesInUtc>
					</QueryOptions>
				]]>
			</CAML>
		</OperationDefinition>
		<OperationDefinition>
			<RequestType>GetListItems</RequestType>
			<ListTitle>MapStageTools</ListTitle>
			<SiteUrl>[SRA Root]</SiteUrl>
			<TableName>MapStageTools</TableName>
			<CAML>
				<![CDATA[
					<ViewFields>
						<FieldRef Name="ID"/>
						<FieldRef Name="Tool"/>
						<FieldRef Name="Stage"/>
						<FieldRef Name="MapName"/>
						<FieldRef Name="ToolID"/>
						<FieldRef Name="MapStageID"/>						
						<FieldRef Name="ToolPosition"/>						
					</ViewFields>
					<QueryOptions>
						<ViewFieldsOnly>True</ViewFieldsOnly>
						<DatesInUtc>True</DatesInUtc>
					</QueryOptions>
				]]>
			</CAML>
		</OperationDefinition>
		<OperationDefinition>
			<RequestType>GetListItems</RequestType>
			<ListTitle>Map</ListTitle>
			<SiteUrl>%SiteURL%</SiteUrl>
			<TableName>LocalMap</TableName>
			<CAML>
				<![CDATA[
					<ViewFields>
						<FieldRef Name="ID"/>
					</ViewFields>
					<QueryOptions>
						<ViewFieldsOnly>True</ViewFieldsOnly>
					</QueryOptions>
				]]>
			</CAML>
		</OperationDefinition>
		<OperationDefinition>
			<RequestType>GetListItems</RequestType>
			<ListTitle>Milestones</ListTitle>
			<SiteUrl>%SiteURL%</SiteUrl>
			<TableName>LocalMilestones</TableName>
			<CAML>
				<![CDATA[
					<ViewFields>
						<FieldRef Name="ID"/>
					</ViewFields>
					<QueryOptions>
						<ViewFieldsOnly>True</ViewFieldsOnly>
					</QueryOptions>
				]]>
			</CAML>
		</OperationDefinition>
		
	</Items>
</OperationDefinitionCollection>
